{
  "compilerOptions": {
    "outDir": "./lib",
    "module": "CommonJS",
    "target": "ES5",
    "lib": [
      "es2015",
      "dom"
    ],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "jsx": "react",
    "strict": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}
